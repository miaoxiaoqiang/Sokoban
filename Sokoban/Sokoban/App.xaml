<Application x:Class="Sokoban.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Sokoban"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Style/Fonts.xaml"/>
                <ResourceDictionary Source="/Resources/Style/Items.xaml"/>
                <ResourceDictionary Source="/Resources/Style/ScrollViewer.xaml"/>
                <ResourceDictionary Source="/Resources/Style/ListBoxItem.xaml"/>
                <ResourceDictionary Source="/Componets/Message/Message.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
